{
    "version": 2,
    "public": true,
    "cleanUrls": true,
    "outputDirectory": "build/web",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!.*\\.js$).*)",
            "destination": "/index.html"
        }
    ]
}